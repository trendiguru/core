name: "VGG_ILSVRC_16_layers_binary_dress"

layer {
      name: "data"
      type: "Python"
      top: "data"
      top: "label"
      python_param {
      module: "jrlayers2"
      layer: "JrMultilabel"
      param_str: "{\'images_and_labels_file\': \'/home/jeremy/image_dbs/tamara_berg_street_to_shop/dress_filipino_labels_train.txt\', \'mean\': (104.0, 116.7, 122.7),\'augment\':True,\'augment_crop_size\':(224,224), \'batch_size\':70 }"
      }
  include {phase: TRAIN}
}
layer {
      name: "data"
      type: "Python"
      top: "data"
      top: "label"
      python_param {
      module: "jrlayers2"
      layer: "JrMultilabel"
      param_str: "{\'images_and_labels_file\': \'/home/jeremy/image_dbs/tamara_berg_street_to_shop/dress_filipino_labels_test.txt\', \'mean\': (104.0, 116.7, 122.7),\'augment\':True,\'augment_crop_size\':(224,224), \'batch_size\':1 }"
     }
   include {phase: TEST}
}


