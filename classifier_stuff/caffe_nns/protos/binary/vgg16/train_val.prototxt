layer {
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1"
  type: "Convolution"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
  param{
    lr_mult: 1
    decay_mult: 1 }
  param{
    lr_mult: 2
    decay_mult: 0 }
}
