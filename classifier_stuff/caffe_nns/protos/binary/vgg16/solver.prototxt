net: "train_val.prototxt"
test_iter: 1000
test_interval: 5000
display: 20
average_loss: 30
weight_decay: 0.0005
# no gradient accumulation
iter_size: 10
snapshot: 2000
snapshot_prefix: "snapshot/vgg16_binary_dress"
test_initialization: false
solver_mode : GPU

#type: "SGD"
type: "Adam"
base_lr: 1e-2
#lr_policy: "step"
lr_policy: "fixed"
gamma: 0.1
stepsize: 10000
max_iter: 3000000
momentum: 0.9
momentum2: 0.999


