name: "VGG_ILSVRC_16_layers_multilabel2"
layer {
      name: "data"
      type: "Python"
      top: "data"
      top: "label"
      python_param {
      module: "jrlayers"
      layer: "JrMultilabel"
#      param_str: "{\'images_and_labels_file\': \'/home/jeremy/image_dbs/tamara_berg/tb_cats_from_webtool_227x227_test.txt\',\'new_size\':(227,227),  \'mean\': (104.00699, 116.66877, 122.67892)}"
