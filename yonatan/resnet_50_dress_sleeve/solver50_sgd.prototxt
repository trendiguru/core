#net: "train_val.prototxt"
train_net: "ResNet-50-train.prototxt"
test_net: "ResNet-50-test.prototxt"
test_iter: 150
test_interval: 200
display: 20
average_loss: 20
weight_decay: 0.0005
# no gradient accumulation
iter_size: 20
snapshot: 5000
snapshot_prefix: "snapshot_50_sgd/train"
test_initialization: false
solver_mode : GPU

type: "SGD"
base_lr: .01
lr_policy: "step"
#lr_policy: "fixed"
gamma: 0.1
stepsize: 5000
max_iter: 100000
momentum: 0.9
